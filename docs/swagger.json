  "components": {
    "schemas": {
      "SaveTestAnswerRequestDto": {
        "type": "object",
        "properties": {
          "answers": {
            "type": "array",
            "description": "질문별 답변 리스트\", example = \"[{\\\"E\\\":4,\\\"I\\\":1}, {\\\"P\\\":3,\"D\":2}]",
            "items": {
              "type": "object",
              "additionalProperties": {
                "type": "integer",
                "format": "int32"
              }
            }
          }
        }
      },
      "UploadRequestDto": {
        "type": "object",
        "properties": {
          "file": {
            "type": "string",
            "format": "binary"
          },
          "folder": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "GroupTeamTypeSettingDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "팀타입 이름",
            "example": "랜덤매칭, 직군별구성"
          },
          "description": {
            "type": "string",
            "description": "팀타입 설명",
            "example": "직무에 따라 팀을 구성하는 방식"
          },
          "startDatetime": {
            "type": "string",
            "format": "date-time",
            "description": "팀 시작 날짜",
            "example": "2025-05-01T00:00:00"
          },
          "endDatetime": {
            "type": "string",
            "format": "date-time",
            "description": "팀 종료 날짜",
            "example": "2025-06-01T00:00:00"
          },
          "minMembers": {
            "type": "integer",
            "format": "int32",
            "description": "최소 팀원 수",
            "example": 3
          },
          "maxMembers": {
            "type": "integer",
            "format": "int32",
            "description": "최대 팀원 수",
            "example": 6
          },
          "positionBased": {
            "type": "boolean",
            "description": "직무 기반 매칭 여부",
            "example": "랜덤매칭=false, 직군별:true"
          },
          "configJson": {
            "type": "string",
            "description": "추가 설정 (JSON 문자열)",
            "example": {
              "option": "value"
            }
          }
        }
      },
      "GroupInviteRequestDto": {
        "type": "object",
        "properties": {
          "groupId": {
            "type": "integer",
            "format": "int32",
            "description": "초대할 그룹 ID",
            "example": 3
          },
          "emails": {
            "type": "array",
            "description": "초대할 사용자 이메일 목록",
            "example": [
              "user1@example.com",
              "user2@example.com"
            ],
            "items": {
              "type": "string"
            }
          }
        }
      },
      "CreateGroupRequestDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "그룹 이름",
            "example": "스위프9기"
          },
